= form_builder.simple_fields_for :credentials do |c|
  = c.input :aws_access_key_id, label: false, placeholder: :translate, wrapper_html: { class: 'aws' }, disabled: provider_blank?
  = c.input :aws_secret_access_key, label: false, placeholder: :translate, wrapper_html: { class: 'aws' }, disabled: provider_blank?
